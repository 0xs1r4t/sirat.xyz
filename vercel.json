{
    "redirects": [
        {
            "source": "/github",
            "destination": "https://github.com/0xs1r4t"
        },
        {
            "source": "/git",
            "destination": "https://git.sirat.xyz"
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "has": [
                {
                    "type": "host",
                    "value": "garden.sirat.xyz"
                }
            ],
            "destination": "/garden"
        },
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "garden.sirat.xyz"
                }
            ],
            "destination": "/garden/:path*"
        }
    ],
    "cleanUrls": true
}
